generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Anime {
  id Int @id @unique
  title String
  title_english String?
  title_romanji String?
  others String?
  Type String
  Status String
  Popularity Float
  Url String
  Genres String[]
  UrlImage String
  Score String
  StartDateYear String
  NbEps Int?
}